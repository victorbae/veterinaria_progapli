<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="460.0" prefWidth="500.0"
	stylesheets="@../../principal/application.css"
	xmlns="http://javafx.com/javafx/9.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="br.com.unoesc.veterinaria.controller.cadastro.ControllerCadastroFuncionario">
	<children>
		<TextField fx:id="tfNome" layoutX="196.0" layoutY="119.0"
			prefHeight="26.0" prefWidth="266.0" styleClass="inputText" />
		<TextField fx:id="tfCpf" layoutX="196.0" layoutY="170.0"
			prefHeight="26.0" prefWidth="266.0" styleClass="inputText" />
		<DatePicker fx:id="dtDataNascimento" layoutX="196.0"
			layoutY="221.0" prefHeight="26.0" prefWidth="266.0"
			styleClass="inputText" />
		<Label layoutX="125.0" layoutY="122.0" styleClass="labelCustom"
			text="Nome:">
			<font>
				<Font name="Gabriola" size="23.0" />
			</font>
		</Label>
		<Label layoutX="132.0" layoutY="173.0" styleClass="labelCustom"
			text="CPF:">
			<font>
				<Font name="Gabriola" size="23.0" />
			</font>
		</Label>
		<Label layoutX="32.0" layoutY="224.0" styleClass="labelCustom"
			text="Data Nascimento:">
			<font>
				<Font name="Gabriola" size="23.0" />
			</font>
		</Label>
		<Label layoutX="79.0" layoutY="275.0" styleClass="labelCustom"
			text="Já é Cliente?">
			<font>
				<Font name="Gabriola" size="23.0" />
			</font>
		</Label>
		<ComboBox fx:id="cbxFilial" layoutX="196.0" layoutY="337.0"
			prefHeight="26.0" prefWidth="266.0" styleClass="inputText" />
		<Label layoutX="139.0" layoutY="340.0" styleClass="labelCustom"
			text="Filial:">
			<font>
				<Font name="Gabriola" size="23.0" />
			</font>
		</Label>
		<Label layoutX="139.0" layoutY="21.0" styleClass="h1"
			text="Novo Funcionário">
			<font>
				<Font name="Gabriola" size="42.0" />
			</font>
		</Label>
		<Button fx:id="btnCancelar" layoutX="238.0" layoutY="412.0"
			mnemonicParsing="false" onAction="#Cancelar" styleClass="btn-danger"
			text="Cancelar" textAlignment="CENTER">
			<font>
				<Font name="Gabriola" size="20.0" />
			</font>
		</Button>
		<Button fx:id="btnSalvar" layoutX="408.0" layoutY="412.0"
			mnemonicParsing="false" onAction="#Salvar" prefWidth="77.0"
			styleClass="btn-success" text="Salvar" textAlignment="CENTER">
			<font>
				<Font name="Gabriola" size="20.0" />
			</font>
		</Button>
		<Button fx:id="btnLimpar" layoutX="324.0" layoutY="412.0"
			mnemonicParsing="false" onAction="#Limpar" prefWidth="77.0"
			styleClass="btn-warning" text="Limpar" textAlignment="CENTER">
			<font>
				<Font name="Gabriola" size="20.0" />
			</font>
		</Button>
		<ComboBox fx:id="cbxCliente" layoutX="196.0" layoutY="272.0"
			prefHeight="26.0" prefWidth="266.0" styleClass="inputText" />
		<Label layoutX="197.0" layoutY="305.0" prefHeight="23.0"
			styleClass="labelMenor" text="Caso não for," />
		<Hyperlink fx:id="ChamaTelaAddCliente" layoutX="270.0"
			layoutY="304.0" onAction="#ChamaTelaAddClienteRapid"
			prefHeight="23.0" styleClass="labelMenor" text="Clique aqui" />
		<Label layoutX="342.0" layoutY="306.0" prefHeight="23.0"
			styleClass="labelMenor" text="para adiciona-lo." />
	</children>
</AnchorPane>
