<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" stylesheets="@../../principal/application.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.unoesc.veterinaria.controller.cadastro.ControllerCadastroTipoAnimal">
   <children>
      <TextField fx:id="tfNome" layoutX="164.0" layoutY="134.0" prefHeight="26.0" prefWidth="266.0" styleClass="inputText" />
      <Label layoutX="104.0" layoutY="136.0" styleClass="labelCustom" text="Nome:">
         <font>
            <Font name="Gabriola" size="23.0" />
         </font>
      </Label>
      <Label layoutX="103.0" layoutY="14.0" styleClass="h1" text="Novo Tipo Animal">
         <font>
            <Font name="Gabriola" size="42.0" />
         </font>
      </Label>
      <Button fx:id="btnCancelar" layoutX="261.0" layoutY="252.0" mnemonicParsing="false" onAction="#cancelar" styleClass="btn-danger" text="Cancelar" textAlignment="CENTER">
         <font>
            <Font name="Gabriola" size="20.0" />
         </font>
      </Button>
      <Button fx:id="btnSalvar" layoutX="422.0" layoutY="252.0" mnemonicParsing="false" onAction="#salvar" styleClass="btn-success" text="Salvar" textAlignment="CENTER">
         <font>
            <Font name="Gabriola" size="20.0" />
         </font>
      </Button>
      <Button fx:id="btnLimpar" layoutX="347.0" layoutY="252.0" mnemonicParsing="false" onAction="#limpar" styleClass="btn-warning" text="Limpar" textAlignment="CENTER">
         <font>
            <Font name="Gabriola" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
